@model Modules.Core.Contracts.UI.Dto.SettingValueDto
@{
	Layout = null;
}

<div class="form-group">
	<label class="col-sm-4 control-label" for="@Model.Id">@Model.Title</label>
	<div class="col-sm-8">
		<input class="form-control" type="text" value="@Model.Value" id="@Model.Id" name="@Model.Id" data-id="setting_@Model.Id"/>
	</div>
</div>